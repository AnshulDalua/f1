{
	"rewrites": [{
		"source": "^/service-worker.js$",
		"destination": "/_next/static/service-worker.js"
	}],
	"headers": [{
			"source": "^/service-worker.js$",
			"headers": [{
					"key": "Cache-Control",
					"value": "public, max-age=43210, immutable"
				},
				{
					"key": "Service-Worker-Allowed",
					"value": "/"
				}
			]
		},
		{
			"source": "/timezone/(.*)",
			"headers": [{
				"key": "Cache-Control",
				"value": "no-cache"
			}]
		}
	]
}